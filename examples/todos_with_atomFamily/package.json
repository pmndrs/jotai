    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-scripts": "4.0.1"
  },
  "devDependencies": {
    "customize-cra": "1.0.0",
    "react-app-rewired": "2.1.6",
    "serve": "11.3.2"
  },
  "resolutions": {
    "webpack": "~4.43.0"
  },
